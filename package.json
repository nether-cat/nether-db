{
  "name": "nether-db",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "npx -n $NODE_DEBUG_OPTION -- vue-cli-service lint --ext \"js,jsx,json,gql,graphql\"",
    "build": "npx -n $NODE_DEBUG_OPTION -- vue-cli-service build",
    "serve": "npx -n $NODE_DEBUG_OPTION -- vue-cli-service serve",
    "apollo": "npx -n $NODE_DEBUG_OPTION -- vue-cli-service apollo:watch --run \"vue-cli-service ssr:serve\"",
    "ssr:serve": "npx -n $NODE_DEBUG_OPTION -- vue-cli-service ssr:serve",
    "ssr:build": "npx -n $NODE_DEBUG_OPTION -- vue-cli-service ssr:build",
    "ssr:start": "cross-env NODE_ENV=production vue-cli-service ssr:serve --mode production",
    "apollo:run": "vue-cli-service apollo:run",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "graphql": "^14.0.2",
    "graphql-type-json": "^0.2.1",
    "isomorphic-fetch": "^2.2.1",
    "lowdb": "^1.0.0",
    "mkdirp": "^0.5.1",
    "register-service-worker": "^1.0.0",
    "shortid": "^2.2.8",
    "vue": "^2.5.17",
    "vue-apollo": "^3.0.0-beta.11",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.16",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@akryum/vue-cli-plugin-ssr": "^0.3.0",
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-e2e-cypress": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.1",
    "@vue/cli-plugin-pwa": "^3.2.0",
    "@vue/cli-plugin-unit-jest": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.26",
    "apollo": "^2.1.7",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.8.0",
    "eslint-plugin-graphql": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0-0",
    "graphql-tag": "^2.9.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-cli-plugin-apollo": "^0.18.1",
    "vue-template-compiler": "^2.5.17"
  }
}
